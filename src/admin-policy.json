[
	{
		"name": "admin-user",
		"selection": {
			"role": {
				"@eq": "ADMIN"
			}
		},
		"config": [{
			"verbs": ["%ALL%"],
			"schema": ["%ALL%"],
			"query": {
				"access": "%FULL_ACCESS%"
			}
		}]
	},
	{
		"name": "admin-lambda-access",
		"selection": {
			"role": {
				"@eq": "ADMIN_LAMBDA"
			}
		},
		"config": [{
			"verbs": "GET",
			"schema": ["app"],
			"query": {
				"id": {
					"@eq": null
				}
			}
		},{
			"verbs": "GET",
			"schema": ["user"],
			"query": {
				"_appId": {
					"@eq": null
				}
			}
		}, {
			"verbs": ["SEARCH"],
			"schema": ["token", "user"],
			"query": [{
				"_appId": {
					"@eq": null
				}
			}]
		}]
	}
]