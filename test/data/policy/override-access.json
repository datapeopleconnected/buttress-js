{
  "name": "override-access",
  "selection": {
    "securityClearance": {
      "@eq": 100
    }
  },
  "config": [
    {
      "verbs": [
        "GET"
      ],
      "schema": [
        "organisation"
      ],
      "env": {
        "switch": {
          "id": "62b09ee325c88db16d9da6ca",
          "state": "ON"
        }
      },
      "condition": {
        "@and": [
          {
            "query.switch": {
              "switch.id": {
                "@eq": "env.switch.id"
              },
              "switch.state": {
                "@eq": "env.switch.state"
              }
            }
          }
        ]
      },
      "query": {
        "access": "%FULL_ACCESS%"
      }
    }
  ]
}