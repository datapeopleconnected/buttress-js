{
  "name": "working-hours",
  "selection": {
    "grade": {
      "@eq": 2
    }
  },
  "config": [
    {
      "verbs": [
        "GET"
      ],
      "schema": [
        "organisation"
      ],
      "env": {
        "startTime": "01:00",
        "endTime": "01:01",
        "time": "now"
      },
      "condition": {
        "@and": [
          {
            "@env.time": {
              "@gtDate": "env.startTime"
            }
          },
          {
            "@env.time": {
              "@ltDate": "env.endTime"
            }
          }
        ]
      },
      "query": {
        "access": "%FULL_ACCESS%"
      }
    }
  ]
}