{
  "name": "working-hours",
  "selection": {
    "grade": {
      "@eq": 2
    }
  },
  "config": [
    {
      "endpoints": [
        "GET"
      ],
      "env": {
        "startTime": "01:00",
        "endTime": "01:01",
        "time": "now"
      },
      "conditions": [
        {
          "schema": [
            "organisation"
          ],
          "@and": [
            {
              "@env.time": {
                "@gtDate": "env.startTime"
              }
            },
            {
              "@env.time": {
                "@ltDate": "env.endTime"
              }
            }
          ]
        }
      ],
      "query": [
        {
          "schema": [
            "organisation"
          ],
          "access": "%FULL_ACCESS%"
        }
      ]
    }
  ]
}