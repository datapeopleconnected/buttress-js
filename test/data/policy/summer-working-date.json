{
  "name": "summer-working-date",
  "priority": 1,
  "merge": true,
  "selection": {
    "grade": {
      "@eq": 6
    }
  },
  "config": [
    {
      "verbs": [
        "GET"
      ],
      "schema": [
        "organisation"
      ],
      "env": {
        "startDate": "01/07/2022",
        "endDate": "31/01/2042",
        "date": "now"
      },
      "condition": {
        "@and": [
          {
            "@env.date": {
              "@gtDate": "env.startDate"
            }
          },
          {
            "@env.date": {
              "@ltDate": "env.endDate"
            }
          }
        ]
      },
      "query": {
        "access": "%FULL_ACCESS%"
      }
    }
  ]
}