{
  "name": "summer-working-hours",
  "priority": 2,
  "merge": true,
  "selection": {
    "securityClearance": {
      "@eq": 1
    }
  },
  "config": [
    {
      "verbs": [
        "GET"
      ],
      "schema": [
        "organisation"
      ],
      "env": {
        "startTime": "01:00",
        "endTime": "01:01",
        "time": "now"
      },
      "condition": {
        "@and": [
          {
            "@env.time": {
              "@gtDate": "env.startTime"
            }
          },
          {
            "@env.time": {
              "@ltDate": "env.endTime"
            }
          }
        ]
      },
      "projection": {
        "keys": [
          "name"
        ]
      }
    }
  ]
}