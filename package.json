  },
  "dependencies": {
    "@buttress/api": "^3.0.0-17",
    "@socket.io/redis-adapter": "~7.2.0",
    "@socket.io/redis-emitter": "~4.1.1",
    "body-parser": "~1.20.0",
    "express": "~4.18.1",
    "JSONStream": "^1.3.5",
    "method-override": "~3.0.0",
    "mongodb": "~4.9.1",
    "morgan": "~1.10.0",
    "mysql2": "^2.3.3",
    "node-env-obj": "~0.2.5",
