  "license": "AGPL-3.0-or-later",
  "private": true,
  "engines": {
    "node": "^14.x.x",
    "npm": "^8.x.x"
  },
  "main": "deploy/app.js",
  "scripts": {
    "test": "mocha test/units/**/* test/intergration/**/*",
    "lint": "eslint 'src/**/*.js'",
    "coverage": "npx nyc --reporter cobertura npm run test",
