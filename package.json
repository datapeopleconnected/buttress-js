{
  "name": "@buttress/buttress-js",
  "version": "3.0.0-1",
  "description": "The federated real-time open data platform",
  "author": "Data Performance Consultancy LTD",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "engines": {
    "node": "^14.x.x",
    "npm": "^8.x.x"
  },
  "main": "deploy/app.js",
  "scripts": {
    "docs": "npx docsify serve ./docs",
    "test": "mocha test/units/**/* test/intergration/**/*",
    "build": "rimraf dist/* && gulp build",
    "watch": "npm run build && gulp watch",
    "lint": "eslint 'src/**/*.js'",
    "coverage": "npx nyc --reporter cobertura npm run test",
    "start": "bin/buttress.sh",
    "docker:build": "docker build -t buttress .",
    "docker:build-token": "npm run docker:build -- --build-arg NPM_TOKEN=${NPM_TOKEN}",
    "docker:run": "docker-compose -f .docker/docker-compose.yml up",
    "docker:run-full": "docker-compose -f .docker/docker-compose.full.yml up"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wearelighten/buttress-js.git"
  },
  "bugs": {
    "url": "https://github.com/wearelighten/buttress-js/issues"
  },
  "dependencies": {
    "@buttress/snippets": "1.0.0-14",
    "@buttress/api": "^3.0.0-31",
    "@hashgraph/sdk": "^2.19.2",
    "@socket.io/redis-adapter": "~7.2.0",
    "@socket.io/redis-emitter": "~4.1.1",
    "base64url": "^3.0.1",
    "body-parser": "~1.20.0",
    "bson-objectid": "^2.0.2",
    "cross-fetch": "^3.1.5",
    "express": "~4.18.1",
    "isolated-vm": "^4.4.2",
    "JSONStream": "^1.3.5",
    "method-override": "~3.0.0",
    "mongodb": "~4.9.1",
    "morgan": "~1.10.0",
    "node-env-obj": "~0.2.5",
    "node-polyfill-webpack-plugin": "2.0.1",
    "node-redis-pubsub": "~5.0.0",
    "object-hash": "^3.0.0",
    "pug": "^3.0.2",
    "randomstring": "^1.2.3",
    "redis": "~3.1.2",
    "rimraf": "^5.0.0",
    "socket.io": "~4.5.1",
    "socket.io-client": "^4.5.1",
    "sugar": "~2.0.6",
    "sugar-inflections": "^2.0.6",
    "typescript": "^5.0.4",
    "url-parse": "1.5.10",
    "uuid": "^8.3.2",
    "webpack": "5.77.0",
    "winston": "~3.8.1"
  },
  "devDependencies": {
    "eslint": "^8.23.0",
    "eslint-config-google": "^0.14.0",
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "mocha": "^9.2.2"
  },
  "mocha": {
    "require": "test/hooks.js"
  }
}
