{
  "name": "@buttress/buttress-js",
  "version": "3.0.0-1",
  "description": "The federated real-time open data platform",
  "author": "Data Performance Consultancy LTD",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "engines": {
    "node": ">=18.18.2"
  },
  "main": "deploy/app.js",
  "scripts": {
    "docs": "npx docsify serve ./docs",
    "test": "npm run build && npm run test:unit && npm run test:e2e",
    "test:unit": "NODE_ENV=test mocha --recursive --parallel test/unit/**/*",
    "test:e2e": "NODE_ENV=test node test/before-e2e.js && echo \"Running BJS in Install Mode.\" && NODE_ENV=test INSTALL_MODE=true ./bin/app.sh && echo \"Install Complete.\\n\"&& NODE_ENV=test TEST_ENV=e2e mocha --recursive --bail test/e2e/index.test.js",
    "build": "rimraf dist/* && gulp build",
    "watch": "npm run build && gulp watch",
    "lint": "eslint 'src/**/*.js'",
    "coverage": "npx nyc --reporter text npm run test",
    "start": "bin/buttress.sh",
    "docker:build": "docker build -t buttress .",
    "docker:build-token": "npm run docker:build -- --build-arg NPM_TOKEN=${NPM_TOKEN}",
    "docker:run": "docker-compose -f .docker/docker-compose.yml up",
    "docker:run-full": "docker-compose -f .docker/docker-compose.full.yml up",
    "prepare": "husky install && ./.husky/nvm-fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wearelighten/buttress-js.git"
  },
  "bugs": {
    "url": "https://github.com/wearelighten/buttress-js/issues"
  },
  "dependencies": {
    "@buttress/api": "~3.0.0-39",
    "@buttress/snippets": "~1.0.0-17",
    "@hashgraph/sdk": "~2.38.0",
    "@socket.io/redis-adapter": "~7.2.0",
    "@socket.io/redis-emitter": "~5.1.0",
    "base64url": "~3.0.1",
    "body-parser": "~1.20.2",
    "bson-objectid": "~2.0.4",
    "cross-fetch": "~4.0.0",
    "express": "~4.18.2",
    "isolated-vm": "~4.6.0",
    "method-override": "~3.0.0",
    "mongodb": "~4.17.1",
    "morgan": "~1.10.0",
    "node-env-obj": "~0.2.5",
    "node-polyfill-webpack-plugin": "~2.0.1",
    "node-redis-pubsub": "~5.0.0",
    "object-hash": "~3.0.0",
    "on-finished": "~2.4.1",
    "pug": "~3.0.2",
    "randomstring": "~1.3.0",
    "redis": "~3.1.2",
    "rimraf": "~5.0.0",
    "socket.io": "~4.7.2",
    "socket.io-client": "~4.7.2",
    "stream-json": "~1.8.0",
    "sugar": "~2.0.6",
    "sugar-inflections": "~2.0.6",
    "typescript": "~5.3.2",
    "url-parse": "~1.5.10",
    "uuid": "~9.0.1",
    "webpack": "~5.89.0",
    "winston": "~3.8.1"
  },
  "devDependencies": {
    "@types/node-redis-pubsub": "^3.0.4",
    "eslint": "^8.54.0",
    "eslint-config-google": "^0.14.0",
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^8.0.3",
    "mocha": "^10.2.0"
  }
}
